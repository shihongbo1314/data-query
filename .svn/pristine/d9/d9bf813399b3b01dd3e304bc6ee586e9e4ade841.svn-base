*{
    margin: 0;
    padding: 0;
}
html,body{
    width: 100%;
    height: 100%;
    font-family: "Microsoft YaHei";
    font-size: 16px;
}
ul{
    list-style: none;
}
/* 谷歌内核滚动条样式 */
::-webkit-scrollbar-thumb {
/*	background: #77A2D7;*/
    background:#C1BEBE;
    border-radius: 99px;
}

::-webkit-scrollbar-track {
    background: #fff;
}

::-webkit-scrollbar {
    width: 4px;
    height: 10px;
}
