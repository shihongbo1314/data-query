                    const treeData = res.data.d.reduce((cur, next) => {
                        const obj = cur.find(
                            (curItem) => curItem.label === next.type
                        );
                        if (obj) {
                            if (obj.children.indexOf(next.text) === -1) {
                                //去重处理
                                obj.children.push({
                                    ...next,
                                    label: next.text,
                                });
                            }
                        } else {
                            const newObj = {
                                label: next.type,
                                id: next.id,
                                children: [
                                    {
                                        ...next,
                                        label: next.text,
                                    },
                                ],
                            };
                            cur.push(newObj);
                        }
                        return cur;
                    }, []);
                    treeData.shift();
                    this.treeData[0].children = [];
                    this.treeData[0].children.push(...treeData);
